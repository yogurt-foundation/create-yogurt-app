  "private": true,
  "main": "index.js",
  "scripts": {
    "gulp": "gulp",
    "watch": "gulp watch",
    "build-dev": "npm run get-frameworks && npm run build-image && npm run build-scripts && npm run build-style && npm run build-html",
    "build-prod": "npm run build-dev && npm run gulp remove-js && npm run gulp remove-css",
    "build-style": "npm run gulp sass && npm run gulp css && npm run gulp purge-css",
    "build-scripts": "npm run gulp pre-scripts && npm run gulp scripts",
    "build-html": "npm run gulp html",
    "build-image": "npm run gulp image",
    "serve": "npm run gulp serve",
    "get-frameworks": "rm -rf ./framework && git clone https://github.com/loouislow81/precompiled-krugurt-core-libs.git ./framework && rm -rf ./framework/.git && cp ./framework/yogurt.min.css ./framework/yogurt.scss"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.3",
    "browser-sync": "^2.26.3",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-concat-css": "^3.1.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^6.1.1",
    "gulp-jsonmin": "^1.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.1.0",
    "gulp-uglify-es": "^2.0.0",
    "gulp-uglifycss": "^1.1.0",
    "gulp-util": "^3.0.8",
    "gulp-webp": "^4.0.1",
